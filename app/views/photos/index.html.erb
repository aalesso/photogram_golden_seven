<h1>List of Photos</h1>

<% @list_of_all_my_photos.each do |photo_singular| %>
<!-- equal sign means evaluate and spit out the result in the html -->
<img src="<%= photo_singular.source %>">
<p>
  <%= photo_singular.caption %>
</p>
<div>
<a href="http://localhost3000/photos/<%= photo_singular.id%>/edit">Edit</a>
<a href="http://localhost3000/delete_photo/<%= photo_singular.id %>">Delete</a>
</div>

<% end %>
